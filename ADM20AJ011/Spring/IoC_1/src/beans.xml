<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- value as element
	<bean id="stud1" class="com.cognizant.spring.Student">
		<property name="name">
		 <value>Sunil</value>
		</property>
	</bean>
-->	
<!-- using p:schema / p-namespace

	<bean id="stud1" class="com.cognizant.spring.Student" p:name="Sunil"></bean>
-->
   	
<!-- value as attribute -->
	<bean id="stud1" class="com.cognizant.spring.Student" lazy-init="true" scope="prototype">
		<property name="name" value="Sunil"/>
	</bean>
   	
   	<bean name="stud2" class="com.cognizant.spring.Student" lazy-init="true">
   		<constructor-arg value="Joseph"></constructor-arg>
   	</bean>
</beans>
